{
    "name": "@slimio/gate",
    "version": "1.0.0",
    "description": "SlimIO Gate built-in addon",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc ava",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Addon-Agent.git"
    },
    "keywords": [
        "SlimIO",
        "monitoring"
    ],
    "files": [
        "index.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Addon-Agent/issues"
    },
    "homepage": "https://github.com/SlimIO/Addon-Agent#readme",
    "dependencies": {
        "@slimio/addon": "^0.22.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^12.0.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.1.0",
        "ava": "^3.8.2",
        "cross-env": "^7.0.2",
        "eslint": "^7.1.0",
        "husky": "^4.2.5",
        "jsdoc": "^3.6.4",
        "nyc": "^15.0.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
