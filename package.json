{
    "name": "@slimio/gate",
    "version": "1.0.0",
    "description": "SlimIO Gate built-in addon",
    "main": "index.js",
    "scripts": {
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc ava",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Addon-Agent.git"
    },
    "keywords": [
        "SlimIO",
        "monitoring"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Addon-Agent/issues"
    },
    "homepage": "https://github.com/SlimIO/Addon-Agent#readme",
    "dependencies": {
        "@slimio/addon": "^0.16.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.2.1",
        "@commitlint/config-conventional": "^7.1.2",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.4",
        "@types/node": "^11.13.8",
        "ava": "^1.4.1",
        "eslint": "^5.16.0",
        "husky": "^2.1.0",
        "jsdoc": "^3.5.5",
        "nyc": "^14.0.0"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
